{
  "name": "@azizuysal/wallet-kit",
  "version": "1.0.0-rc.1",
  "description": "A React Native wrapper for Apple and Google wallet passes",
  "source": "./src/index.tsx",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace @azizuysal/wallet-kit-example",
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "prepare": "bob build",
    "release": "release-it",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "docs:markdown": "typedoc --plugin typedoc-plugin-markdown --out docs/api-markdown",
    "generate-jwt": "node scripts/generate-jwt.js"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "wallet",
    "apple-wallet",
    "google-wallet",
    "passkit",
    "pkpass",
    "passes",
    "mobile-wallet",
    "digital-wallet",
    "wallet-pass",
    "react-native-wallet",
    "add-to-wallet",
    "payment",
    "tickets",
    "boarding-pass",
    "loyalty-card"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azizuysal/wallet-kit.git"
  },
  "author": "Aziz Uysal (https://github.com/azizuysal)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/azizuysal/wallet-kit/issues"
  },
  "homepage": "https://github.com/azizuysal/wallet-kit#readme",
  "security": {
    "email": "security@azizuysal.com",
    "url": "https://github.com/azizuysal/wallet-kit/blob/main/SECURITY.md"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@evilmartians/lefthook": "^1.5.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native/eslint-config": "^0.73.1",
    "@release-it/conventional-changelog": "^9.0.2",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.44",
    "@types/react-test-renderer": "^19.1.0",
    "commitlint": "^17.0.2",
    "conventional-changelog-cli": "^5.0.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-native-builder-bob": "^0.35.2",
    "react-test-renderer": "18.3.1",
    "release-it": "^17.10.0",
    "turbo": "^2.5.6",
    "typedoc": "^0.28.11",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@3.6.1",
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "\\.d\\.ts$"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|@react-navigation)/)"
    ],
    "moduleNameMapper": {
      "^react-native$": "<rootDir>/node_modules/react-native"
    },
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/__tests__/**",
      "!src/**/*.test.{ts,tsx}",
      "!src/**/*.spec.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 85,
        "statements": 85
      },
      "./src/WalletButton.tsx": {
        "branches": 95,
        "functions": 100,
        "lines": 100,
        "statements": 100
      },
      "./src/index.tsx": {
        "branches": 75,
        "functions": 80,
        "lines": 85,
        "statements": 85
      }
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}",
      "requireCleanWorkingDir": true,
      "requireBranch": "main"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native",
      "prettier"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/",
    "coverage/",
    "docs/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "esm": true
        }
      ]
    ]
  },
  "create-react-native-library": {
    "type": "legacy-module",
    "languages": "kotlin-objc",
    "version": "0.45.5"
  }
}
